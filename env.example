# Firebase Configuration - Environment Variables Example
# Copy this file to .env.local for development or .env.production for production
# Replace the placeholder values with your actual Firebase configuration

# Firebase API Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id_here

# Application Configuration
VITE_APP_NAME=Israel Cyber Academy
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Security Configuration
VITE_ENABLE_SECURITY_LOGGING=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance Configuration
VITE_ENABLE_CACHING=true
VITE_CACHE_TTL=300000

# Development Configuration (only for development)
VITE_ENABLE_DEBUG_LOGGING=true
VITE_ENABLE_FIREBASE_EMULATORS=false 